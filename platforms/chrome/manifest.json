{
    "manifest_version": 2,
    "name": "Odometer",
    "description": "距離計",
    "version": "0.1.0",
    "app": {
        "background": {
            "scripts": [
                "background.js"
            ]
        }
    },
    "icons": {
        "16": "icon_16.png",
        "48": "icon_48.png",
        "128": "icon.png"
    },
    "permissions": [
        "clipboardWrite",
        "clipboardRead",
        {
            "fileSystem": [
                "write",
                "retainEntries",
                "directory"
            ]
        },
        "unlimitedStorage",
        "geolocation",
        "notifications"
    ]
}
